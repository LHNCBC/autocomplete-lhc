{
  "name": "autocomplete-lhc",
  "version": "18.4.0",
  "description": "",
  "main": "source/index.js",
  "directories": {
    "test": "test"
  },
  "commentsOnDeps": {},
  "peerDependencies": {
    "jquery": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "bower": "^1.8.8",
    "clean-css-cli": "^4.2.1",
    "connect": "^3.4.1",
    "cypress": "^9.5.2",
    "grunt": "^1.4.1",
    "grunt-cli": "^1.2.0",
    "grunt-protractor-runner": "^5.0.0",
    "jasmine-spec-reporter": "^2.4.0",
    "jit-grunt": "^0.10.0",
    "mocha": "^5.2.0",
    "serve-index": "^1.7.3",
    "serve-static": "^1.10.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "wiredep": "^4.0.0"
  },
  "scripts": {
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update --versions.chrome $(google-chrome --version |awk '{print $NF}')",
    "test": "test/run_tests.sh && cypress run",
    "test:e2e": "test/run_tests.sh skip_unit_tests && cypress run",
    "build:clean": "rm -rf dist",
    "build:mkdist": "mkdir dist; mkdir dist/$npm_package_name-$npm_package_version; (cd dist; ln -s $npm_package_name-$npm_package_version latest)",
    "build:css": "cleancss --skip-rebase -o dist/latest/autocomplete-lhc.min.css source/auto_completion.css",
    "build:cp": "cp source/*png LICENSE.md README-dist.md dist/latest",
    "build:zip": "cd dist && zip -r autocomplete-lhc-$npm_package_version.zip $npm_package_name-$npm_package_version",
    "build": "npm run build:clean && npm run build:mkdist && webpack && npm run build:css && npm run build:cp && npm run build:zip"
  },
  "repository": "github:lhncbc/autocomplete-lhc",
  "license": "See LICENSE.md",
  "dependencies": {}
}
